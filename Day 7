class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        if not nums:
            return 0

        L = 0 #its te index where the next unique element will be placed

        #R is te explorer looking across the array for elements
        for R in range(1, len(nums)):
            #if unique value is found
            if nums[R]!= nums[L]:
                L += 1
                nums[L] = nums[R]

        #return the mosot unique no of elements
        return L + 1
