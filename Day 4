class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        if not strs:
            return ""

        #we start with first word as reference
        #i goes across columns - column index - runs horizantally 
        for i in range(len(strs[0])):
            char = strs[0][i]

            #j goes down the rows - row index - runs vertically
            for j in range(1, len(strs)):
                if i==len(strs[j]) or strs[j][i]!=char:
                    return strs[0][:i]
